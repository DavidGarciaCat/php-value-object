{
    "name": "david-garcia/value-object",
    "description": "Just another library to handle Value Objects and prevent the bad habit of Primitive Obsession",
    "type": "library",
    "authors": [
        {
            "name": "David Garcia",
            "email": "me@davidgarcia.cat"
        }
    ],
    "require": {
        "php": ">= 7.4",
        "egulias/email-validator": "^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "DavidGarcia\\ValueObject\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DavidGarcia\\Tests\\ValueObject\\": "tests/"
        }
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    }
}
